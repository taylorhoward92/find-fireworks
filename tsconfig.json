{
  "compilerOptions": {
    "composite": true,
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "module": "NodeNext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "preserveValueImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022",
  }
}
  